  Theme.unavailableText = "{% comment %}ly_i18n_replace_for_['products.product.unavailable' | t | escape ]_begin{% endcomment %}{% capture ly_template %}{{'products.product.unavailable' | t | escape }}{% endcapture %}{% include 'ly-static-string' with '1713948' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape | escape }}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}";
  Theme.processingText = "{% comment %}ly_i18n_replace_for_[ 'products.product.processing' | t | escape ]_begin{% endcomment %}{% capture ly_template %}{{ 'products.product.processing' | t | escape }}{% endcapture %}{% include 'ly-static-string' with '1713949' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape | escape }}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}";
  Theme.pleaseSelectText = "{% comment %}ly_i18n_replace_for_[ 'general.general.please_select' | t | escape ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.general.please_select' | t | escape }}{% endcapture %}{% include 'ly-static-string' with '1713950' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape | escape }}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}";

  {% if shop.customer_accounts_enabled and customer %}
    Theme.customerLoggedIn = true;
  {% endif %}

  {% if settings.shipping-calculator and cart.requires_shipping %}
    Theme.shippingCalculator = true;
    Theme.shippingButton = "{% comment %}ly_i18n_replace_for_[ 'cart.shipping_calculator.submit' | t | escape ]_begin{% endcomment %}{% capture ly_template %}{{ 'cart.shipping_calculator.submit' | t | escape }}{% endcapture %}{% include 'ly-static-string' with '1713951' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape | escape }}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}";
    Theme.shippingDisabled = "{% comment %}ly_i18n_replace_for_[ 'cart.shipping_calculator.disabled' | t | escape ]_begin{% endcomment %}{% capture ly_template %}{{ 'cart.shipping_calculator.disabled' | t | escape }}{% endcapture %}{% include 'ly-static-string' with '1713952' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape | escape }}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}";

    {% if shop.customer_accounts_enabled and customer and customer.addresses.size > 0 %}
      Theme.customerAddress = {{ customer.default_address | json }};
    {% endif %}

    Theme.shippingCalcErrorMessage = "{% comment %}ly_i18n_replace_for_[ 'cart.shipping_calculator.error' | t | escape ]_begin{% endcomment %}{% capture ly_template %}{{ 'cart.shipping_calculator.error' | t | escape }}{% endcapture %}{% include 'ly-static-string' with '1713953' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape | escape }}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}";
    Theme.shippingCalcMultiRates = "{% comment %}ly_i18n_replace_for_[ 'cart.shipping_calculator.rates_html' | t | escape ]_begin{% endcomment %}{% capture ly_template %}{{ 'cart.shipping_calculator.rates_html' | t | escape }}{% endcapture %}{% include 'ly-static-string' with '1713954' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape }}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}";
    Theme.shippingCalcOneRate = "{% comment %}ly_i18n_replace_for_[ 'cart.shipping_calculator.rate' | t | escape ]_begin{% endcomment %}{% capture ly_template %}{{ 'cart.shipping_calculator.rate' | t | escape }}{% endcapture %}{% include 'ly-static-string' with '1713955' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape | escape }}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}";
    Theme.shippingCalcNoRates = "{% comment %}ly_i18n_replace_for_[ 'cart.shipping_calculator.no_rates' | t | escape ]_begin{% endcomment %}{% capture ly_template %}{{ 'cart.shipping_calculator.no_rates' | t | escape }}{% endcapture %}{% include 'ly-static-string' with '1713956' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape | escape }}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}";
    Theme.shippingCalcRateValues = "{% comment %}ly_i18n_replace_for_[ 'cart.shipping_calculator.rate_values_html' | t | escape ]_begin{% endcomment %}{% capture ly_template %}{{ 'cart.shipping_calculator.rate_values_html' | t | escape }}{% endcapture %}{% include 'ly-static-string' with '1713957' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape }}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}";
  {% endif %}

  {% if settings.currency-switcher %}
    Theme.currencySwitcher = true;
    Theme.currencySwitcherFormat = '{{ settings.currency-switcher-format | escape }}';
    /*
      Override the default Theme.moneyFormat for recalculating
      prices on the product and cart templates.
    */
    Theme.moneyFormat = {{ shop.[settings.currency-switcher-format] | json }};
  {% endif %}

  {% if settings.product-zoom %}
    Theme.imageZoom = true;
  {% endif %}

  {% if settings.slideshow %}
    Theme.slideshow = true;

    {% if settings.slideshow-autoplay %}
      Theme.slideshowAutoplay = true;
      Theme.slideshowAutoplayDelay = 4;
        {% if settings.slideshow-autoplay-delay != blank %}
          Theme.slideshowAutoplayDelay = parseInt('{{ settings.slideshow-autoplay-delay | escape }}',10);
        {% endif %}
    {% endif %}
  {% endif %}

  {% if settings.instagram-widget and settings.instagram-widget-access-token != blank %}
    Theme.instagram = true;
    Theme.instagramAccessToken = '{{ settings.instagram-widget-access-token | escape }}';
    Theme.instagramShowTag = {{ settings.use-instagram-tag | json }};
    Theme.instagramTag = '{{ settings.instagram-widget-tag | escape }}';
  {% endif %}

  {% if settings.twitter-widget and settings.twitter-widget-id != blank %}
    Theme.twitter = true;
    Theme.twitterId = '{{ settings.twitter-widget-id | escape }}';
    Theme.twitterShowRetweets = {{ settings.twitter-widget-retweets | json }};
  {% endif %}

  Theme.itemRemovedFromCart = "{% comment %}ly_i18n_replace_for_[ 'cart.general.item_removed_message' | t | escape ]_begin{% endcomment %}{% capture ly_template %}{{ 'cart.general.item_removed_message' | t | escape }}{% endcapture %}{% include 'ly-static-string' with '1713958' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape | escape }}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}";
  Theme.itemRemovedFromCartUndo = "{% comment %}ly_i18n_replace_for_[ 'cart.general.undo' | t | escape ]_begin{% endcomment %}{% capture ly_template %}{{ 'cart.general.undo' | t | escape }}{% endcapture %}{% include 'ly-static-string' with '1713959' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape | escape }}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}";

  {% if settings.sticky-header %}
    Theme.stickyHeader = true;
  {% endif %}

  Theme.cartText = "{% comment %}ly_i18n_replace_for_[ 'layout.header.cart' | t | escape ]_begin{% endcomment %}{% capture ly_template %}{{ 'layout.header.cart' | t | escape }}{% endcapture %}{% include 'ly-static-string' with '1713960' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape | escape }}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}";
  Theme.itemRemovedFromCart = "{% comment %}ly_i18n_replace_for_[ 'cart.general.item_removed_message' | t | escape ]_begin{% endcomment %}{% capture ly_template %}{{ 'cart.general.item_removed_message' | t | escape }}{% endcapture %}{% include 'ly-static-string' with '1713958' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape | escape }}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}";

  Theme.quickShop = {% if settings.product-hover == "quick-shop" %}true{% else %}false{% endif %};
  Theme.relatedProducts = {{ settings.related-products | json }};
  Theme.featuredProducts = {{ settings.featured-products | json }};

  // For selecting the first available variant
  var FirstVariant = {};
</script>
